{
    "name": "deerig/envalert",
    "description": "This package will show on the top of every page a fancy alert with the environment and the current branch information.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeeRig\\EnvAlert\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeeRig\\EnvAlert\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeeRig\\EnvAlert\\EnvAlertServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "DeeRig",
            "email": "contact@deerig.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
