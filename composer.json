{
    "name": "deerig/envbar",
    "description": "This package will show on the top of every page a fancy alert with the environment and the current branch information.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeeRig\\EnvBar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeeRig\\EnvBar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeeRig\\EnvBar\\EnvBarServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "DeeRig",
            "email": "contact@deerig.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
